{
  "name": "@mizushino/firestore-orm",
  "version": "0.0.6",
  "description": "TypeScript-first ORM library for Firestore with support for both client-side (web) and server-side (admin) environments",
  "type": "module",
  "main": "./dist/admin/index.js",
  "module": "./dist/admin/index.js",
  "types": "./dist/admin/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./web": {
      "types": "./dist/web/index.d.ts",
      "default": "./dist/web/index.js"
    },
    "./admin": {
      "types": "./dist/admin/index.d.ts",
      "default": "./dist/admin/index.js"
    },
    "./shared/utils": {
      "types": "./dist/shared/utils.d.ts",
      "default": "./dist/shared/utils.js"
    },
    "./shared/types": {
      "types": "./dist/shared/types.d.ts",
      "default": "./dist/shared/types.js"
    },
    "./shared/async-queue": {
      "types": "./dist/shared/async-queue.d.ts",
      "default": "./dist/shared/async-queue.js"
    }
  },
  "files": [
    "dist",
    "shared",
    "web",
    "admin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "emulator": "firebase emulators:start --project=demo-firestore-orm --config=test/firebase.json",
    "test:admin": "tsx test/admin/test.ts",
    "test:web": "tsx test/web/test.ts"
  },
  "keywords": [
    "firestore",
    "orm",
    "firebase",
    "firebase-admin",
    "typescript",
    "activerecord",
    "repository",
    "web-components"
  ],
  "author": "mizushino",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mizushino/firestore-orm.git"
  },
  "bugs": {
    "url": "https://github.com/mizushino/firestore-orm/issues"
  },
  "homepage": "https://github.com/mizushino/firestore-orm#readme",
  "peerDependencies": {
    "firebase": ">=12.0.0",
    "firebase-admin": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "firebase": {
      "optional": true
    },
    "firebase-admin": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "firebase-tools": "^14.20.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.1"
  }
}
